[
  { "@method":"load-me",
    "args":{},
    "code":[
      { "@define": ["token", "code/text/word", {
        "S": "自分が",
        "O": "自分を"
      }] },
      { "@call": "array.push",
        "arguments": {
          "to": { "@arg":"current" },
          "value": { "@var":"token" }
        }
      },
      { "@goto":"hook" }
    ]
  },
