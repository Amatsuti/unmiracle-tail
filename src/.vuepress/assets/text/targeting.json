  { "@method":"serious",
    "args":{ "HP":500 },
    "code":[
      { "@call": "number.to-string",
        "arguments": {
          "to": { "@var":"HP", "as":"string" },
          "value": { "@calc":"@HP / 10" }
        }
      },
      { "@call": "string.format",
        "arguments": {
          "to": { "@var":"fmt", "as":"string" },
          "map": {
            "HP": { "@var":"HP" }
          },
          "value": "HP{HP}%以下の"
        }
      },
      { "@define": ["token", "code/text/word", {
        "M": { "@var":"fmt" }
      }] },
      { "@call": "array.push",
        "arguments": {
          "to": { "@arg":"current" },
          "value": { "@var":"token" }
        }
      },
      { "@goto":"hook" }
    ]
  },
  { "@method":"unequip-buff",
    "args":{
      "buffCommand": "test-cmd/buff-heal"
    },
    "code":[
      { "@call": [{"@arg":"buffCommand"}, "add"],
        "arguments": {
          "buff-name": { "@var":"name", "as":"string" }
        }
      },
      { "@call": "string.format",
        "arguments": {
          "to": { "@var":"fmt", "as":"string" },
          "map": {
            "name": { "@var":"name" }
          },
          "value": "{name}状態でない"
        }
      },
      { "@define": ["token", "code/text/word", {
        "M": { "@var":"fmt" }
      }] },
